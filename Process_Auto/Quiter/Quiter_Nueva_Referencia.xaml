  <Sequence sap2010:Annotation.AnnotationText="Extrae la ruta donde se alojan los archivos temporales y obtiene el ultimo pdf descargado, para tomar el numero de referencia generado por Quiter." DisplayName="Quiter_Nueva_Referencia" sap:VirtualizedContainerService.HintSize="375.2,950.4" sap2010:WorkflowViewState.IdRef="Sequence_1">
    <Sequence.Variables>
      <Variable x:TypeArguments="x:String" Name="RutaTemporal" />
      <Variable x:TypeArguments="x:String" Name="RutaArchivo" />
      <Variable x:TypeArguments="x:String" Name="TextoArchivo" />
    </Sequence.Variables>
    <sap:WorkflowViewStateService.ViewState>
      <scg:Dictionary x:TypeArguments="x:String, x:Object">
        <x:Boolean x:Key="IsExpanded">True</x:Boolean>
        <x:Boolean x:Key="IsAnnotationDocked">True</x:Boolean>
      </scg:Dictionary>
    </sap:WorkflowViewStateService.ViewState>
    <Assign DisplayName="Asignar ruta temporal a variable" sap:VirtualizedContainerService.HintSize="333.6,60" sap2010:WorkflowViewState.IdRef="Assign_1">
      <Assign.To>
        <OutArgument x:TypeArguments="x:String">[RutaTemporal]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:String">[in_Config("RutaDescargaPDF").ToString]</InArgument>
      </Assign.Value>
    </Assign>
    <Assign DisplayName="Obtener ruta del archivo" sap:VirtualizedContainerService.HintSize="333.6,60" sap2010:WorkflowViewState.IdRef="Assign_2">
      <Assign.To>
        <OutArgument x:TypeArguments="x:String">[RutaArchivo]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:String">[String.Join("parameter",Directory.GetFiles(RutaTemporal,"*.pdf").OrderByDescending(Function(d) New FileInfo(d).CreationTime).Take(1))]</InArgument>
      </Assign.Value>
    </Assign>
    <ui:ReadPDFWithOCR DegreeOfParallelism="-1" DisplayName="Leer último archivo descargado" FileName="[RutaArchivo]" sap:VirtualizedContainerService.HintSize="333.6,190.4" sap2010:WorkflowViewState.IdRef="ReadPDFWithOCR_1" ImageDpi="150" Range="All" Text="[TextoArchivo]">
      <ui:ReadPDFWithOCR.OCREngine>
        <ActivityFunc x:TypeArguments="sd:Image, scg:IEnumerable(scg:KeyValuePair(sd:Rectangle, x:String))">
          <ActivityFunc.Argument>
            <DelegateInArgument x:TypeArguments="sd:Image" Name="Image" />
          </ActivityFunc.Argument>
          <ui:MicrosoftOCR FilterRegion="{x:Null}" Language="{x:Null}" Output="{x:Null}" Profile="{x:Null}" Scale="{x:Null}" Text="{x:Null}" ComputeSkewAngle="False" DisplayName="Lectura de texto en PDF" ExtractWords="False" sap:VirtualizedContainerService.HintSize="283.2,80.8" sap2010:WorkflowViewState.IdRef="MicrosoftOCR_1" Image="[Image]" />
        </ActivityFunc>
      </ui:ReadPDFWithOCR.OCREngine>
    </ui:ReadPDFWithOCR>
    <Assign DisplayName="Obtener Número de referencia del texto del archivo" sap:VirtualizedContainerService.HintSize="333.6,60" sap2010:WorkflowViewState.IdRef="Assign_3">
      <Assign.To>
        <OutArgument x:TypeArguments="x:String">[out_NumeroReferencia]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:String">[Split(Split(TextoArchivo,"REFERENCIA")(1)," ")(0)]</InArgument>
      </Assign.Value>
    </Assign>
